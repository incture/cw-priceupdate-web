<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l='sap.ui.layout'>
	<ResponsivePopover id='inctureMDNotifications'
		placement="Bottom" title="" contentWidth='22rem' contentHeight='20rem'>
		<content>
			<IconTabBar class='inctureMDITBClass'>
				<items>
					<IconTabFilter text='Activity Log'>
						<List items='{/notifications}' class="inctureMDNotificationClass">
							<items>
								<NotificationListItem description='{msg}'
									datetime='{date}' showButtons='false' authorName=""
									authorPicture="{icon}" />
							</items>
						</List>
					</IconTabFilter>
					<IconTabFilter text='Alerts On'>
						<List items='{/alerts}' class="inctureMDNotificationClass">
							<items>
								<NotificationListItem description='{msg}'
									datetime='{date}' showButtons='false' authorName=""
									authorPicture="{icon}" />
							</items>
						</List>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</content>
	</ResponsivePopover>
</core:FragmentDefinition>

