<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified.CalendarLegend" xmlns:html="http://www.w3.org/1999/xhtml">
	<Dialog contentWidth='80%' showHeader="false">
		<customHeader class="legendImage">
        	<Bar>
            	<contentLeft>
               		<Text text="The new record overlaps with an existing condition and will cause it to split/delimit. \n Select an option to proceed." class="textFontStyle"></Text>
            	</contentLeft>
            	<contentRight>
               		<Image src="images/legend1.png" class="legendImage"></Image>
            	</contentRight>
       		</Bar>
    	</customHeader>		
		<beginButton>
			<Button text="OK" press="onConfirmDialog"></Button>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>