<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="PU_Reports.controller.Master"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:tnt="sap.tnt" xmlns:l="sap.ui.layout">
	<Page title="Master" class="incMasterPage" showHeader="false">
		<content>
			<l:Grid span='S12' vSpacing='0'>
				<VBox>
					<Image src="./images/placeholder.png" class="incMRoundBorderRadiusClass inctureMDMargin0_5remClass" width="4rem" height="4rem"></Image>
					<layoutData>
						<l:GridData spanS='12' visibleL='false' visibleXL='false' visibleM='false' visibleS='true'></l:GridData>
					</layoutData>
				</VBox>
				<StandardListItem title='User' description='' class='inctureMDUserDetailsClass'>
					<layoutData>
						<l:GridData spanS='12' visibleL='false' visibleXL='false' visibleM='false' visibleS='true'></l:GridData>
					</layoutData>
				</StandardListItem>
			</l:Grid>
			<List items="{oListReportsModel>/reportsList}" itemPress="onListItemSelect" class="inctureMDMasterListClass">
				<StandardListItem title="{oListReportsModel>reportName}" iconInset="false" type="Navigation" class="inctureMDMasterListItemClass"/>
			</List>
		</content>
	</Page>
</mvc:View>