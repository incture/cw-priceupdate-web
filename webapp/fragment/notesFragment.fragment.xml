<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog title="Notes" class="dashboardNotesHeaderClass">
		<TextArea maxLength="500" rows="4" cols="50" class="dashboardNotesTextareaStyle">
			
		</TextArea>
		<buttons>
			<Button text="Cancel" type="Reject" press="onNotesCancel"/>
			<Button text="Save" type="Accept" press="onNotesSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>